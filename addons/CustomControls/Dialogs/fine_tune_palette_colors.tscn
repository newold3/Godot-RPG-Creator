[sub_resource type="Shader" id="Shader_8chai"]
code = "shader_type canvas_item;

// --- MODOS ---
uniform bool colorize_mode = false;

// [0]=Master, [1]=Rojos, ...
uniform float hue_offsets[7];

uniform float target_hue; // 0.0 a 1.0
uniform float target_sat; // 0.0 a 1.0

vec3 hsv2rgb(vec3 c) {
    vec4 K = vec4(1.0, 2.0 / 3.0, 1.0 / 3.0, 3.0);
    vec3 p = abs(fract(c.xxx + K.xyz) * 6.0 - K.www);
    return c.z * mix(K.xxx, clamp(p - K.xxx, 0.0, 1.0), c.y);
}

int get_channel_id(float hue_norm) {
    float h_deg = hue_norm * 360.0;
    if (h_deg >= 325.0 || h_deg < 35.0) return 1;
    if (h_deg >= 35.0 && h_deg < 95.0) return 2;
    if (h_deg >= 95.0 && h_deg < 155.0) return 3;
    if (h_deg >= 155.0 && h_deg < 215.0) return 4;
    if (h_deg >= 215.0 && h_deg < 275.0) return 5;
    if (h_deg >= 275.0 && h_deg < 325.0) return 6;
    return 0;
}

void fragment() {
    float base_hue = UV.x;
    
    if (colorize_mode) {
        vec3 rgb = hsv2rgb(vec3(target_hue, target_sat, 1.0));
        COLOR = vec4(rgb, 1.0);
    } else {
        int ch_id = get_channel_id(base_hue);
        float total_shift = hue_offsets[0];
        
        if (ch_id > 0) {
            for(int i = 1; i <= 6; i++) {
                if (i == ch_id) total_shift += hue_offsets[i];
            }
        }
        
        float final_hue = mod(base_hue + total_shift, 1.0);
        vec3 rgb = hsv2rgb(vec3(final_hue, 1.0, 1.0));
        COLOR = vec4(rgb, 1.0);
    }
}"

